      },
      "source": [
        "num_trigger_tokens = 3\n",
        "trigger_token_ids = [c_text.vocab.stoi[\"the\"]] * num_trigger_tokens\n",
        "trigger_token_ids"
      ],
      "execution_count": 0,
      "outputs": [
        {
          "output_type": "execute_result",
